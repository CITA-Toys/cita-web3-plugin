{
  "name": "@cita/web3-plugin",
  "version": "1.0.27",
  "keywords": [
    "CITA",
    "Ethereum",
    "JSONRPC",
    "Promise"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.js",
  "repository": "https://github.com/Keith-CY/cita-web3-plugin",
  "author": "Keith <keithwhisper@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "protoc": "protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --proto_path=src/cita-proto --ts_out=./src/cita-proto --js_out=import_style=commonjs,binary:./src/cita-proto src/cita-proto/blockchain.proto",
    "prepublish": "yarn run test && yarn run build && cp -r ./src/cita-proto ./lib"
  },
  "devDependencies": {
    "@types/node": "^9.6.1",
    "jest": "^22.4.3",
    "ts-protoc-gen": "^0.7.3",
    "typescript": "^2.8.1"
  },
  "dependencies": {
    "@types/google-protobuf": "^3.2.7",
    "axios": "^0.18.0",
    "google-protobuf": "^3.5.0"
  }
}
